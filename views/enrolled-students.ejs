<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Introduction to Sequelize</title>

    <link href="../../../css/bootstrap.min.css" rel="stylesheet">

  </head>
  <body>
<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">

          <h1 class="text-center"><%= enrolled_data[0].subject.subject_name %></h1>


        <center>
          <a href="/subjects"><button type="button" name="button" class="btn btn-primary"> Subjects List </button></a>
        </center>


          <table class="table table-striped table-hover">
            <thead>
              <th class="text-center">No</th>
              <th>Students Name</th>
              <th class="text-center">Score</th>
              <th class="text-center">Desc</th>
            </thead>
            <% var i = 1; enrolled_data.forEach(getData => { %>
              <tr>
                <td class="text-center">
                  <%= i %><% i += 1 %>
                </td>
                <td>
                  <%= getData.student.firstname + ' ' + getData.student.lastname %>
                </td>
                <td class="text-center">
                  <% if(getData.score == undefined){%>
                    <a href="/subjects/<%= getData.student.id %>/<%= getData.subject.id %>/<%= getData.subject.subject_name %>/give-score">
                      <button type="button" name="button" class="btn btn-primary">Give Score</button>
                    </a>
                  <% } else { %>
                    <%= getData.score %>
                  <%}%>
                </td>
                <td class="text-center">
                  <%= nilai[i-2] %>
                </td>
              </tr>

            <% }) %>

          </table>

    </div>
  </div>
</div>
    <script src="../js/jquery.min.js"></script>
    <script src="../js/bootstrap.min.js"></script>
  </body>
</html>
